$theme: '';.sc-theme .sc-segmented-view {
  display: block;
}

.sc-theme .sc-segment:active, .sc-segment:focus {
	outline: none;
}

.sc-theme .sc-segment img.icon {
	position: relative;
	vertical-align: middle;
	top: -2px;
	margin-right: 2px;
	height: 16px;
	width: 16px;
}

.sc-theme .sc-segmented-view.disabled .sc-segment img.icon {
	opacity: 0.5;
	-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=50)"; 
  filter: progid:DXImageTransform.Microsoft.Alpha(Opacity=50);
  -moz-opacity: 0.5;
}



.sc-theme .sc-segmented-view .sc-segment {
	display: inline-block;
	top: 0;
	height: 24px;
	line-height: 24px;
	text-shadow: #fff 0px 1px 0px;
	margin-left: -1px;
	text-decoration: none ;
	color: black ;
	text-align: center ;
}

.sc-theme .sc-segmented-view .sc-button-inner {
  display: block;
  min-width: 20px;
  min-height: 24px;
  border: none;
}

.sc-theme .sc-segmented-view .sc-segment {
	padding-left: 11px;
	padding-right: 0;
	border: none;
	background:  transparent;
}


.sc-theme .sc-segmented-view .sc-segment .sc-button-inner {
  padding-left: 0;
  padding-right: 11px;
  background:  transparent;
}

.sc-theme .sc-segmented-view .sc-segment label {
  display: block;
  line-height: 24px;
  height:  24px;
  text-align: center;
  background:  transparent;
}







.sc-theme .sc-segmented-view .sc-segment.sc-last-segment .sc-button-inner {
